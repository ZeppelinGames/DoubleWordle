{
    "name": "DoubleWordle",
    "description": "Show if a letter occurs more than once in Wordle",
    "version": "1.0.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["*://www.nytimes.com/games/wordle/*"],
            "js": ["/injection/js/doubleLetter.js"]
        }
    ]
}